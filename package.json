{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "lint": "eslint resources/js/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix 'resources/js/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'resources/js/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "js-watch": "npm run lint:fix && npm run format && npm run watch"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "axios": "^0.21",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14",
    "prettier": "^3.0.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.1",
    "@shopify/polaris": "^11.26.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.16.0",
    "yup": "^1.3.2"
  }
